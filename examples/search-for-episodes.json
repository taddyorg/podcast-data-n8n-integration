    {
      "parameters": {
        "jsCode": "let episodeUuids = [];\n\n// Loop over input items\nfor (const item of $input.all()) {\n  // Check if podcastEpisodes exists and has episodes\n  if (item.json.podcastEpisodes && item.json.podcastEpisodes.length > 0) {\n    // Extract UUIDs from all episodes in this item\n    for (const episode of item.json.podcastEpisodes) {\n      if (episode.uuid) {\n        episodeUuids.push(episode.uuid);\n      }\n    }\n  }\n}\n\n// Return the episode UUIDs as separate items for the next node\nreturn { json: { episodeUuids: episodeUuids.join(',') } };"
      },
